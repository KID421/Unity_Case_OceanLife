[{"strDataType":"text","strName":"中文名","strValue":"巨大海星","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Pisaster giganteus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"海盤車科 (Asteriidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"巨大海星","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Giant sea star","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"61 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於受保護的海岸及低潮帶上，常見其附著於岩石上或碼頭的木樁以及沙地上。產卵季節在每年的3月到4月。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"一般個體具有五隻腕足，腕足寬大，體表具有縝密的骨板，體表佈滿粗壯的鈍刺，顏色多為藍白色、粉紅色或紫色。口面具有管足分布於腕足中央並匯集於口器，顏色較背面淺，多為藍色，深藍，碇色等，周圍並帶有棕色的纖毛。叉棘結構為鉗狀。","strThumbnailUrl":null},{"strDataType":"image","strName":"巨大海星","strValue":"http://image.nmmba.gov.tw/upload/archive/23681/preview_20161212094414.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23681/thumbnail_20161212094414.jpg"}]