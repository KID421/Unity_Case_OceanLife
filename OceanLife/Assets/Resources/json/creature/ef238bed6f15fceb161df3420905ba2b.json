[{"strDataType":"text","strName":"中文名","strValue":"尖吻鱸","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Lates calcarifer","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"尖吻鱸科 (Latidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"金目鱸、扁紅目鱸、網槽","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Palmer、 Sea bass、 Silver perch、Silver barramundi、White sea bass、 Asian seabass、 Cock-up, Barramundi perch、 Barramundi、 Giant seaperch、 Giant perch、 Giant sea-perch、Giant sea perch","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"200 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"西部、南部","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至西太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"為熱帶及亞熱帶沿岸海域魚類，棲息於岩岸礁石與泥沙交匯處，而常活動於半淡鹹水水域，亦會溯入淡水河川。屬廣鹽性魚類且不耐低溫。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體長而側扁，腹緣平直。頭尖，頭背側、眼睛上方有一明顯凹槽。眼略小。前後鼻孔相近。吻短而略尖。口大，下頜略突出；上頜後端達眼後下方。眶前骨與前鰓蓋骨均無鋸齒之雙重邊緣，前鰓蓋骨下緣3-4硬棘。上、下頜、鋤骨、腭骨及翼骨上均有絨毛狀齒帶，舌上無齒。被細小櫛鱗；側線鱗數60-63。背鰭單一，具深缺刻，鰭條數為VII-VIII+I, 11；臀鰭鰭條數為III+8；胸鰭短；尾鰭圓形。成魚體呈銀白色，體背側灰褐至藍灰色，各鰭灰黑或淡色；幼魚褐色至灰褐色，頭部具3條白紋，體側散布白色斑紋，眼褐色至金黃色，隨成長而略具淡紅色虹彩。","strThumbnailUrl":null},{"strDataType":"video","strName":"尖吻鱸","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1874/90254ca4c6d55694b23e2781af4fd8b8.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1874/preview.jpg"},{"strDataType":"image","strName":"970.\t尖吻鱸（扁紅眼鱸） Lates calcarifer (Bloch, 1770)","strValue":"http://image.nmmba.gov.tw/upload/archive/21538/preview_20160604120604.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/21538/thumbnail_20160604120604.jpg"},{"strDataType":"image","strName":"尖吻鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23926/preview_20161214084940.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23926/thumbnail_20161214084940.jpg"},{"strDataType":"image","strName":"尖吻鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23927/preview_20161214084948.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23927/thumbnail_20161214084948.jpg"},{"strDataType":"image","strName":"尖吻鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23928/preview_20161214084954.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23928/thumbnail_20161214084954.jpg"},{"strDataType":"image","strName":"","strValue":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/080_S001.jpg","strThumbnailUrl":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/080_S001.jpg"},{"strDataType":"image","strName":"","strValue":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/080_S001.jpg","strThumbnailUrl":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/080_S001.jpg"}]