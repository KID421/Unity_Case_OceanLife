[{"strDataType":"text","strName":"中文名","strValue":"斑鰭光鰓雀鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Chromis punctipinnis","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"雀鯛科 (Pomacentridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"地中海至東大西洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於海底，礁岩、海底斜坡或海藻林等水域，深度可達46公尺深。夜行性，白天多躲藏於礁岩石縫中，對於其他魚種具有強烈的領域性。食性主要以藻類或浮游動物為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體長側扁，鱗片大，尾鰭分岔，背鰭硬棘XIII，軟條11-12，臀鰭硬棘II，軟條10-12。體色多為黑藍色並帶有黑色小點在尾端。幼魚體色呈現頭端為藍灰色，尾端為棕色至橘色。","strThumbnailUrl":null},{"strDataType":"video","strName":"斑鰭光鰓雀鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2067/dd63acb0ac185bca84ee37843cc958f9.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2067/preview.jpg"},{"strDataType":"image","strName":"斑鰭光鰓雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23235/preview_20161104134820.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23235/thumbnail_20161104134820.jpg"},{"strDataType":"image","strName":"斑鰭光鰓雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23236/preview_20161104134839.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23236/thumbnail_20161104134839.jpg"}]