[{"strDataType":"text","strName":"中文名","strValue":"斑帶副鱸","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Paralabrax maculatofasciatus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鮨科 (Serranidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"60 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於沙地或泥地水域以及海草床中。食性已小魚和底漆甲殼類為食。對於水溫適應性及大，可適應溫度介於攝氏7.5-32度。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈紡錘狀，側扁，頭尖，口大，前鰓蓋具有鋸齒，胸鰭硬棘X，軟條13-14，第三硬棘明顯較長，較第二硬棘三倍長。臀鰭硬棘III，軟條6-8，胸鰭16，側線鱗片61-68。體色多為白色帶黑色、棕色或橙色斑點，斑點在腹部後方較為密集，體側帶有6至7條暗帶，眼睛到鰓蓋有一條傾斜的黑線花紋。幼魚有一條黑線自眼睛延伸到尾鰭，尾端和尾鰭則有密集的黑點。","strThumbnailUrl":null},{"strDataType":"video","strName":"斑帶副鱸","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2059/12134659a3907696f18c401b6efab967.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2059/preview.jpg"},{"strDataType":"image","strName":"斑帶副鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23710/preview_20161212104356.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23710/thumbnail_20161212104356.jpg"},{"strDataType":"image","strName":"斑帶副鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23990/preview_20161214105834.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23990/thumbnail_20161214105834.jpg"},{"strDataType":"image","strName":"斑帶副鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23991/preview_20161214105838.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23991/thumbnail_20161214105838.jpg"}]