[{"strDataType":"text","strName":"中文名","strValue":"環吻琵琶鱝","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Rhinobatos productus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"琵琶鱝科 (Rhinobatidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"119 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於海灣或河口的沙地或泥地以及鄰近的礁岩地區，會單獨行動亦會群體行動。晝伏夜出，日間會潛沙休息，夜間覓食，食性已螃蟹、雙殼類、多毛類、小魚為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體長，體後半部狀似鯊魚，胸鰭至頭部形成扁平的三角形結構，吻端軟骨形成兩條山脊狀突起，幼魚的吻端具有流蘇般的瓣膜，鰓孔位於眼後，尾鰭上下不相等，尾鰭上部較下部長。","strThumbnailUrl":null},{"strDataType":"video","strName":"環吻琵琶鱝","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2087/d4426374d1be5cb73db34d4d20666518.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2087/preview.jpg"},{"strDataType":"image","strName":"環吻琵琶鱝","strValue":"http://image.nmmba.gov.tw/upload/archive/24066/preview_20161219115608.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24066/thumbnail_20161219115608.jpg"},{"strDataType":"image","strName":"環吻琵琶鱝","strValue":"http://image.nmmba.gov.tw/upload/archive/24067/preview_20161219115617.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24067/thumbnail_20161219115617.jpg"}]