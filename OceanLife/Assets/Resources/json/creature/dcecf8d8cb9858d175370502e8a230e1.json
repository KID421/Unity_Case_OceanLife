[{"strDataType":"text","strName":"中文名","strValue":"東太平洋頭鯊","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Cephaloscyllium ventriosum","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"貓鯊科 (Scyliorhinidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"110 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息分布於大陸棚或大陸坡上層水域，常出現在帶有藻類的礁岩地形。食性以硬骨魚或魚屍為食，也會食用甲殼類。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體長略呈紡錘狀，背鰭相較一般鯊魚為接近尾鰭，鰓裂小，開合幅度小，眼大，呈現金色，牙齒數量介於55至60顆，皆為小齒。","strThumbnailUrl":null},{"strDataType":"video","strName":"東太平洋頭鯊","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2258/b92dc0b16ddea57482d96536be27a249.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2258/preview.jpg"},{"strDataType":"image","strName":"東太平洋頭鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23939/preview_20161214093747.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23939/thumbnail_20161214093747.jpg"},{"strDataType":"image","strName":"東太平洋頭鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23940/preview_20161214093758.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23940/thumbnail_20161214093758.jpg"},{"strDataType":"image","strName":"東太平洋頭鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23941/preview_20161214093802.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23941/thumbnail_20161214093802.jpg"},{"strDataType":"image","strName":"東太平洋頭鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23942/preview_20161214093812.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23942/thumbnail_20161214093812.jpg"}]