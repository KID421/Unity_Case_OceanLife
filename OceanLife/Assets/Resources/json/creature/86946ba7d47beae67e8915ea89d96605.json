[{"strDataType":"text","strName":"中文名","strValue":"福氏江魟","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Potamotrygon falkneri","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"江魟科 (Potamotrygonidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"60 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"南美洲","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"淡水魟魚，棲息於河流的砂質淺灘，多於河床底部移動。在河水氾濫時常會隨著河水移動，當洪水退去後時常會出現於該地區的湖泊或池塘。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體型扁平，圓盤狀，尾鰭根部粗壯，較體長。尾鰭根部上方具有一不規則淡斑點，體色多為深色系，具有多種花紋，如斑點或網點狀花紋。","strThumbnailUrl":null},{"strDataType":"video","strName":"福氏江魟","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1941/8df841f28f8bbee17be34d5bc808246f.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1941/preview.jpg"},{"strDataType":"image","strName":"福氏江魟","strValue":"http://image.nmmba.gov.tw/upload/archive/23680/preview_20161212094408.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23680/thumbnail_20161212094408.jpg"}]