[{"strDataType":"text","strName":"中文名","strValue":"螫裸胸鯙","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Gymnothorax mordax","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鯙科 (Muraenidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"152 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於淺海域，藏隱身於岩洞或石縫中，僅露出頭部。食性主要以甲殼類或小型魚類為主。壽命長。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體長呈圓柱狀，柔軟有彈性，不具鱗片和側線。無背鰭、胸鰭和臀鰭，脊椎骨數146-154。體色多為深褐色、淡褐色、綠色，並帶有縝密的斑紋。部分區域的個體有較大的牙齒。","strThumbnailUrl":null},{"strDataType":"video","strName":"螫裸胸鯙","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2075/f4594780851379735a8ca52682d5ecbe.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2075/preview.jpg"},{"strDataType":"image","strName":"螫裸胸鯙","strValue":"http://image.nmmba.gov.tw/upload/archive/24068/preview_20161219115626.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24068/thumbnail_20161219115626.jpg"},{"strDataType":"image","strName":"螫裸胸鯙","strValue":"http://image.nmmba.gov.tw/upload/archive/24069/preview_20161219115633.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24069/thumbnail_20161219115633.jpg"},{"strDataType":"image","strName":"螫裸胸鯙","strValue":"http://image.nmmba.gov.tw/upload/archive/24070/preview_20161219115641.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24070/thumbnail_20161219115641.jpg"}]