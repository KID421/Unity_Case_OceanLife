[{"strDataType":"text","strName":"中文名","strValue":"柴魚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Microcanthus strigatus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"舵魚科 (Kyphosidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"斑馬、條紋蝶","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Stripey, Convict fish, Butterflyfish","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"16 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、西部、北部、東北部、澎湖、小琉球","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於岩岸礁石區或潟湖區，亦常見於潮地內。雜食性，以海藻及底棲動物為食。 ","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體高而側扁，呈長卵形。頭小而吻尖。眼較大。口小，前位，口裂近水平。頜齒多行，尖細，呈刷毛狀；鋤骨、腭骨均無齒。側線完全，側線鱗數56-60。背鰭硬棘X-XI，軟條數17-18；臀鰭III，軟條數14-16；尾鰭微凹。體一致為黃色，體側具5條微斜的黑色縱帶，最上方之縱帶由背鰭起點延伸至背鰭中央軟條部之後緣；最下方之縱帶由吻端貫通眼徑延伸至臀鰭起點。背、腹及臀鰭黃色；背及臀鰭上亦有黑色縱帶；胸鰭淡黃色；尾鰭淡色。","strThumbnailUrl":null},{"strDataType":"video","strName":"柴魚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2044/720d59099b3f4052b10e05afc9354c79.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2044/preview.jpg"},{"strDataType":"image","strName":"柴魚","strValue":"http://image.nmmba.gov.tw/upload/archive/12002/preview_20110919140240.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/12002/thumbnail_20110919140240.jpg"},{"strDataType":"image","strName":"柴魚","strValue":"http://image.nmmba.gov.tw/upload/archive/12003/preview_20110919140254.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/12003/thumbnail_20110919140254.jpg"},{"strDataType":"image","strName":"柴魚","strValue":"http://image.nmmba.gov.tw/upload/archive/12004/preview_20110919140309.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/12004/thumbnail_20110919140309.jpg"},{"strDataType":"image","strName":"1514.\t細刺魚（柴魚） Microcanthus strigatus (Cuvier, 1831)","strValue":"http://image.nmmba.gov.tw/upload/archive/22281/preview_20160815094421.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22281/thumbnail_20160815094421.jpg"},{"strDataType":"image","strName":"柴魚","strValue":"http://image.nmmba.gov.tw/upload/archive/23708/preview_20161212104343.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23708/thumbnail_20161212104343.jpg"},{"strDataType":"image","strName":"","strValue":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/060619135847 Microcanthus strigatus.jpg","strThumbnailUrl":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/060619135847 Microcanthus strigatus.jpg"}]