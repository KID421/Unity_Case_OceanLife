[{"strDataType":"text","strName":"中文名","strValue":"半藍刻齒雀鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Chrysiptera hemicyanea","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"雀鯛科 (Pomacentridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"藍雀","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"azure damselfish, azure demoiselle, half-blue demoiselle, yellow-dipped damsel","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"7 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至西太平洋，東印度洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"成體棲息於潟湖或近岸珊瑚礁，通常小群體聚集在分支型珊瑚之間，以水底層約30公分至50公分的浮游動物為食，繁殖期間，雌雄各自成對，雌魚會將卵產至堅硬的基質，雄魚則有護卵行為如以煽動魚卵確保水流交換和抵禦外敵。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"背鰭硬棘XIII，軟條10-12，臀鰭硬棘II，軟條11-12。","strThumbnailUrl":null},{"strDataType":"video","strName":"半藍刻齒雀鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2050/736a3a9e32b810156866f4377c447231.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2050/preview.jpg"},{"strDataType":"image","strName":"半藍刻齒雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23719/preview_20161212111728.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23719/thumbnail_20161212111728.jpg"},{"strDataType":"image","strName":"半藍刻齒雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23720/preview_20161212111735.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23720/thumbnail_20161212111735.jpg"}]