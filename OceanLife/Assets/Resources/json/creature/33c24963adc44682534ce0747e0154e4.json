[{"strDataType":"text","strName":"中文名","strValue":"肝色裸胸鯙","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Gymnothorax hepaticus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鯙科 (Muraenidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"錢鰻、薯鰻、虎鰻","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Liver-colored moray eel","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"100 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"南部、澎湖","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"西太平洋  ","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於較深的沿岸水域。肉食性，伺機捕食魚類或頭足類為生，偶食甲殼類。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體延長而呈圓柱狀，尾部側扁；尾長約較頭及軀幹略長或相等。吻較短，且較不彎曲。背鰭與臀鰭發育正常，前者起點在鰓孔以前，後鼻孔為短管狀，或者邊緣平坦無突出。脊椎骨數171-181。體一致呈灰褐色至茶褐色，背鰭及臀鰭具白緣；70公分以下之魚體於背鰭前之頭背部具暗色鞍狀斑。","strThumbnailUrl":null},{"strDataType":"image","strName":"209.\t白邊裸胸鱔 Gymnothorax hepaticus (Rüppell, 1830)","strValue":"http://image.nmmba.gov.tw/upload/archive/20704/preview_20160217164648.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/20704/thumbnail_20160217164648.jpg"}]