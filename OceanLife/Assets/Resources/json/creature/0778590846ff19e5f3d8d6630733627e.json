[{"strDataType":"text","strName":"中文名","strValue":"白帶固曲齒鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Plectroglyphidodon leucozonus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"雀鯛科 (Pomacentridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"12 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、西部、南部、北部、東北部、澎湖、小琉球、蘭嶼、綠島、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於海岸線的湧浪區與臨海礁石的邊緣。稚魚時常出現在潮間帶礁石頂冠的口袋區，或掩蔽的海灣，或淺水域礁砂混合區內大圓石礁上方；而成魚被發現於在洶湧的峽道之間的脊坡，或清澈水域內的溝槽與海峭壁上緣礁石上方。以底藻為食 。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈卵圓形而側扁，標準體長為體高之1.8-1.9倍。吻短而略尖。眼中大，上側位。口小，上頜骨末端不及眼前緣；齒單列，且較長。眶下骨具鱗，後緣則平滑；眶前骨與眶下骨間無缺刻；前鰓蓋骨後緣平滑。體被櫛鱗；側線之有孔鱗片19-20個。背鰭單一，軟條部不延長而呈角形，硬棘XII，軟條15-16；臀鰭硬棘II，軟條12-13；胸鰭鰭條19-20；尾鰭叉形，末端呈尖形，上下葉外側鰭條不延長呈絲狀。體呈暗青銅色，每個鱗片後方具一淡色點及暗色緣；體側中部具一白色橫帶。稚魚在白色橫帶後面的背鰭基底上有一個顯著的眼狀斑。","strThumbnailUrl":null},{"strDataType":"video","strName":"白帶固曲齒鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2004/5e29c73ed1bdbf0547aa6848357cfd8a.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2004/preview.jpg"},{"strDataType":"image","strName":"白帶固曲齒鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/9889/preview_20101013164008.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/9889/thumbnail_20101013164008.jpg"},{"strDataType":"image","strName":"白帶固曲齒鯛 ","strValue":"http://image.nmmba.gov.tw/upload/archive/14616/preview_20120924104625.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/14616/thumbnail_20120924104625.jpg"},{"strDataType":"image","strName":"白帶固曲齒鯛 ","strValue":"http://image.nmmba.gov.tw/upload/archive/14617/preview_20120924104636.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/14617/thumbnail_20120924104636.jpg"}]