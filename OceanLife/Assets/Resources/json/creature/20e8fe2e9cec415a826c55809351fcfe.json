[{"strDataType":"text","strName":"中文名","strValue":"佛氏異齒鯊","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Heterodontus francisci","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"異齒鯊科 (Heterodontidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"122 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於礁岩、海藻床、或砂質地形海域，常躲藏於岩縫或石洞中，夜行性，有固定棲所，因此會在夜間外出狩獵後在日間回到棲所休息。食性已海膽、螃蟹、螺貝類等無脊椎生物為食。個性溫和，但受到驚擾食仍會以牙齒攻擊敵人。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈圓柱狀，尾部略側扁。頭部寬短，吻端較鈍，眼眶上方略有突起，頭骨低於眼眶。缺少一般鯊魚具有的瞬膜。上顎具19-26齒列，下顎具18-29齒列，牙齒形狀尖銳而小。","strThumbnailUrl":null},{"strDataType":"image","strName":"佛氏異齒鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23937/preview_20161214092023.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23937/thumbnail_20161214092023.jpg"},{"strDataType":"image","strName":"佛氏異齒鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23938/preview_20161214092030.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23938/thumbnail_20161214092030.jpg"}]