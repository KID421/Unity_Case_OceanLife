[{"strDataType":"text","strName":"中文名","strValue":"太平洋粗唇海鯽","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Rhacochilus vacca","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"海鯽科 (Embiotocidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"44.2 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於礁岩海岸、海藻林以及水下掩體。食性以軟體動物、甲殼類及藤壺為食。雌魚具有育幼的行為。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈紡錘狀，側扁，頭尖，背鰭硬棘IX-XI，軟條21-25，臀鰭硬棘III，軟條27-30，脊椎骨數33-38。體色為帶有金屬光澤的銀灰色，其中體側和腹部及背部帶有灰黑色或棕色的的斑點。幼魚帶有黑色縱帶，會隨著年紀成長逐漸變淡。","strThumbnailUrl":null},{"strDataType":"video","strName":"太平洋粗唇海鯽","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2052/d987d0b2a047be00b4adfe8b6f6371fc.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2052/preview.jpg"},{"strDataType":"image","strName":"太平洋粗唇海鯽","strValue":"http://image.nmmba.gov.tw/upload/archive/23899/preview_20161213165013.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23899/thumbnail_20161213165013.jpg"},{"strDataType":"image","strName":"太平洋粗唇海鯽","strValue":"http://image.nmmba.gov.tw/upload/archive/23900/preview_20161213165019.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23900/thumbnail_20161213165019.jpg"},{"strDataType":"image","strName":"太平洋粗唇海鯽","strValue":"http://image.nmmba.gov.tw/upload/archive/23901/preview_20161213165030.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23901/thumbnail_20161213165030.jpg"}]