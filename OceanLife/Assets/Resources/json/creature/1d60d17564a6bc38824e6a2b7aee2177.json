[{"strDataType":"text","strName":"中文名","strValue":"馬可羅尼企鵝","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Eudyptes chrysolophus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"企鵝科 (Spheniscidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Macaroni penguin","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"70 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"南美、非洲南端及亞南極島嶼","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於極地區域，雌雄外觀相近，雄性個體體型較大，食性以磷蝦、小魚及頭足類為食。每年換羽一次，離水生活約3-4周，，會用石頭堆砌自己的巢穴在夏季離水育幼後，分散返回海中巡迴近6個月。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈橢圓形，頭部有黃色裝飾羽，長度自眼前延伸至頸部，成熟個體的嘴喙長約7-8公分，高約1公分。翅膀從肩膀到末端約20.4公分，尾部長度約9-10公分，頭部至頸部及背部羽毛為黑色，腹部羽毛則為白色，兩色之間界線分明。新生羽毛會帶有藍色金屬光澤，舊羽則為棕色。鰭狀翅膀上部羽毛為藍黑色，下部為白色，且外緣顏色與背部顏色相同。眼珠虹膜為紅色，眼皮及周圍皮膚為粉紅色。腳和腿同樣皮膚為粉紅色。","strThumbnailUrl":null},{"strDataType":"video","strName":"企鵝池","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2231/ab958ae90854e9fac3820987ace12515.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2231/preview.jpg"},{"strDataType":"video","strName":"馬可羅尼企鵝","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2233/61b539e023cb17ef5188a14c59c99716.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2233/preview.jpg"},{"strDataType":"image","strName":"馬可羅尼企鵝","strValue":"http://image.nmmba.gov.tw/upload/archive/7404/preview_007404.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/7404/thumbnail_007404.jpg"},{"strDataType":"image","strName":"馬可羅尼企鵝","strValue":"http://image.nmmba.gov.tw/upload/archive/7409/preview_007409.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/7409/thumbnail_007409.jpg"}]