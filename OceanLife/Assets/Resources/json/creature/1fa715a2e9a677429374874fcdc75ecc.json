[{"strDataType":"text","strName":"中文名","strValue":"大管孔珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Goniopora djiboutiensis","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"孔珊瑚科 (Poritidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"大管孔珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Flowerpot coral","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東北部、東部、南部、澎湖、小琉球、綠島、蘭嶼、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至西太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"通常分布於水深介於5-20公尺深的珊瑚礁平臺或斜坡。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"珊瑚群體呈柱形或團塊形，珊瑚孔略呈圓形，口杯直徑介於3-5公釐，有明顯中柱，珊瑚蟲呈管狀，長度介於5-10公分，不會內縮，觸手中央有錐狀突起。珊瑚群體顏色多呈白色。","strThumbnailUrl":null},{"strDataType":"image","strName":"管孔珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/3577/preview_003577.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/3577/thumbnail_003577.jpg"},{"strDataType":"image","strName":"Goniopora djiboutiensis‭ ‬大管孔珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/7988/preview_20090422160403.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/7988/thumbnail_20090422160403.jpg"},{"strDataType":"image","strName":"大管孔珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/8575/preview_20100318140105.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/8575/thumbnail_20100318140105.jpg"}]