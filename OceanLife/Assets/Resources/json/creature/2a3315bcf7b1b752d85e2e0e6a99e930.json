[{"strDataType":"text","strName":"中文名","strValue":"細點裸胸鯙","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Gymnothorax pictus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鯙科 (Muraenidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"錢鰻、薯鰻、虎鰻","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Peppered moray","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"140 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、南部、澎湖、小琉球、蘭嶼、綠島","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於沿岸珊瑚、岩礁隙縫中；經常將挺起的身體前半部露出洞穴外。活動敏捷，常外出獵食；喜歡捕食甲殼類或魚類。 ","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"魚體較為延長；吻部較鈍。牙齒呈圓錐狀，鋤骨齒二排。體色及斑紋隨年齡增加而有變異。脊椎骨數127-133。成魚體底色白，且上滿布褐色不定形斑點，頭部之斑點小於眼徑，身體部分則或聚集成大塊碎斑；幼魚之底色為白，其上布有眼徑大小之 C型黑斑，不規則地排成若干縱列，且隨成長而列數增加，腹部前較白。","strThumbnailUrl":null},{"strDataType":"image","strName":"222.\t細點裸胸鱔 Gymnothorax pictus (Ahl, 1789)","strValue":"http://image.nmmba.gov.tw/upload/archive/20717/preview_20160217170004.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/20717/thumbnail_20160217170004.jpg"}]