[{"strDataType":"text","strName":"中文名","strValue":"勃氏刻齒雀鯛 ","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Chrysiptera brownriggii","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"雀鯛科 (Pomacentridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"厚殼仔","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Surge demoiselle","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"8 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、南部、東北部、澎湖、小琉球、蘭嶼、綠島、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於碎石底的洶湧峽道、裸露礁石平臺的最外側及海底上側之臺地，棲息深度在12公尺內。 具有領域性，但是通常成群生活, 而且都逗留在庇護所附近。 主要以底部的藻類與小型甲殼動物為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈橢圓形而側扁，標準體長為體高之2.3-2.5倍。眼中大，上側位。口小，上頜骨末端僅及眼前緣。眶下骨裸出；前鰓蓋骨後緣平滑。頰部有鱗3列。體被櫛鱗；側線之有孔鱗片18-19個。鰓耙數19-21。背鰭單一，軟條部不延長而呈角形，硬棘XIII，軟條12-15；臀鰭硬棘II，軟條12-13；胸鰭鰭條17-19；尾鰭叉形，上下葉末端呈角形。幼魚體呈黃褐色，眼後上方延伸至尾柄背面具一條藍色帶，背鰭硬棘部後半部上具一眼斑，隨著成長而減小，成魚則完全消失；成魚體呈暗褐色，鰓蓋後方具一黃色橫帶，體側中央及尾柄上各具一條白色橫帶。","strThumbnailUrl":null},{"strDataType":"video","strName":"勃氏刻齒雀鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2024/e1010205d5d89e07f3858b2bb8eb3186.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2024/preview.jpg"},{"strDataType":"image","strName":"布氏刻齒雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/11459/preview_20150121101113.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/11459/thumbnail_20150121101113.jpg"},{"strDataType":"image","strName":"布氏刻齒雀鯛 ","strValue":"http://image.nmmba.gov.tw/upload/archive/12029/preview_20110926154251.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/12029/thumbnail_20110926154251.jpg"},{"strDataType":"image","strName":"布氏刻齒雀鯛 ","strValue":"http://image.nmmba.gov.tw/upload/archive/18880/preview_20150316140431.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/18880/thumbnail_20150316140431.jpg"},{"strDataType":"image","strName":"1671.\t布氏金翅雀鯛 Chrysiptera brownriggii (Bennett, 1829)","strValue":"http://image.nmmba.gov.tw/upload/archive/22747/preview_20160919084105.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22747/thumbnail_20160919084105.jpg"},{"strDataType":"image","strName":"","strValue":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/113.jpg","strThumbnailUrl":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/113.jpg"},{"strDataType":"image","strName":"","strValue":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/113.jpg","strThumbnailUrl":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/113.jpg"}]