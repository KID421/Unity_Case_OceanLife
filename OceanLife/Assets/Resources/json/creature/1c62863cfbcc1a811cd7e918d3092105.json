[{"strDataType":"text","strName":"中文名","strValue":"大西洋海刺水母","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Chrysaora quinquecirrha","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"遠洋水母科 (Pelagiidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Atlantic sea nettle","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"40 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"大西洋、印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"常被發現於沿岸淺水域、河口或港口等地。比大西洋水母體型在稍小，在美東是常見的水母。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體成圓傘狀，體腹具有細長的觸手以及外緣和口腕有細小的觸手，觸手帶有刺絲胞，可藉此獵捕小型生物。體色呈半透明，帶有白色的斑點或紅棕色的條紋。","strThumbnailUrl":null},{"strDataType":"video","strName":"大西洋海刺水母","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2035/131c4c28ddb3e7916520afcd9b4ee90b.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2035/preview.jpg"},{"strDataType":"image","strName":"大西洋海刺水母","strValue":"http://image.nmmba.gov.tw/upload/archive/23677/preview_20161212093725.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23677/thumbnail_20161212093725.jpg"},{"strDataType":"image","strName":"大西洋海刺水母","strValue":"http://image.nmmba.gov.tw/upload/archive/23678/preview_20161212093733.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23678/thumbnail_20161212093733.jpg"},{"strDataType":"image","strName":"大西洋海刺水母","strValue":"http://image.nmmba.gov.tw/upload/archive/23679/preview_20161212093740.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23679/thumbnail_20161212093740.jpg"}]