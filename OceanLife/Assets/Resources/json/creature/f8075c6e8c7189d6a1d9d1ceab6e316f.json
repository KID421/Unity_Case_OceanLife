[{"strDataType":"text","strName":"中文名","strValue":"斑龜","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Ocadia sinensis","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"地龜科","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"綠龜、花龜、中華花龜、青頭龜","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Chinese striped-neck turtle","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"北部、中部、南部、東部、西部","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"中國、臺灣及越南","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於鄰近淡水水源的區域，如水流較緩慢的溪流、溝渠、塘埤、水庫等。物種分布位於熱帶及亞熱帶地區，因此沒有冬眠的習性，僅於氣溫低於攝氏15度時候躲藏不動。食性為雜食性，水生昆蟲及動物或植物皆為此物種食物。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"頭部至頸部有明顯薑黃和黑色的縱向條紋，四肢也有同樣的花紋。背殼橢圓，顏色由紅棕色轉黑，緣盾末端以及腹甲為黃色，腹甲又具有黑色塊斑。雌龜體型較雄龜大，雄龜在腹甲末端略凸。","strThumbnailUrl":null},{"strDataType":"video","strName":"斑龜","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2229/186d9ffd87d850f2eb153b761bdf080e.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2229/preview.jpg"},{"strDataType":"video","strName":"斑龜","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2247/ea1f79a780ce2bfce81cfa35d17f7978.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2247/preview.jpg"},{"strDataType":"image","strName":"斑龜","strValue":"http://image.nmmba.gov.tw/upload/archive/5848/preview_005848.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/5848/thumbnail_005848.jpg"}]