[{"strDataType":"text","strName":"中文名","strValue":"波紋海螺","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Megastraea undosa","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"蠑螺科 (Turbinidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Wavy turban snail","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"14.5 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於潮上帶、潮池或礁岩塊下。以刮食岩石上的藻類為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"殼近圓錐狀，殼厚堅實。螺塔低平約4-5層，縫合線不深。殼表有許多細顆粒相連成的螺肋。縫合線下方的螺肋向外扭曲成波浪狀。殼表成棕色。殼口橢圓形，外唇簡單，無臍孔。石灰質口蓋形狀特殊，呈橢圓狀，從其一尖端有四條成放射狀的肋脊。","strThumbnailUrl":null}]