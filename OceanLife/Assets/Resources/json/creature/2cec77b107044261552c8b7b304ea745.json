[{"strDataType":"text","strName":"中文名","strValue":"黃紋鼓氣鱗魨","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Sufflamen fraenatum","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鱗魨科 (Balistidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"假面砲彈、黃紋板機魨","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Triggerfish","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"38 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、南部、北部、東北部、綠島、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於砂岩混合之向海礁區。主要以甲殼類、軟體動物、蠕蟲、被囊動物、卵及碎屑等為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體稍延長，呈長橢圓形， 尾柄短。口端位，齒白具缺刻。眼前有一深溝。頰部被鱗；鰓裂後有大型骨質鱗片。尾柄鱗片具小棘列，且向前延伸至身體中央，第一背鰭下方。背鰭兩個，基底相接近，第一背鰭位於鰓孔上方，第I棘粗大，第II棘則細長，第III背鰭棘明顯；背鰭及臀鰭軟條截平；尾鰭弧形。體深褐色；雄魚在口角有一白帶向後延伸至胸鰭基前方，雌魚無此色帶；除胸鰭褐色外，餘鰭皆為深褐色。","strThumbnailUrl":null},{"strDataType":"image","strName":"黃紋鼓氣鱗魨","strValue":"http://image.nmmba.gov.tw/upload/archive/4125/preview_20150828105532.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4125/thumbnail_20150828105532.jpg"}]