[{"strDataType":"text","strName":"中文名","strValue":"無齒鰺","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Gnathanodon speciosus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鰺科 (Carangidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"虎斑瓜","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Trevally, Yellow jack fish, Yellow ulua, Golden trevally, King fish, Golden jack, Golden toothless trevally, Golden trevally, Golden horsmackerel, Golden kingfish, Golden fish","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"110 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"西部、南部、北部、東北部、澎湖、綠島、東沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"幼魚常游動於鯊魚或其他大型魚旁以撿食碎屑並獲得保護，有「領航魚」之稱。成魚則巡游於沿岸礁石區、瀉湖區，以其厚唇在沙地中尋找無脊椎動物，如甲殼類、軟體動物或小魚等。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈橢圓形，側扁而高。眼小。脂性眼瞼不發達。上頜骨末端僅達眼前緣下方。上頜、鋤骨、腭骨及舌面皆無齒，下頜亦僅於幼魚時有數枚微小的細齒，成魚則消失。體被小圓鱗，胸部完全具鱗。側線直走部始於第二背鰭第8-9鰭條之下方。稜鱗弱，只存在直走部的後半部。第二背鰭與臀鰭同形，前方鰭條呈新月形，後方不具有離鰭；體色銀白至黃色，體側有7-11條寬窄相間的黑色橫帶，第一條橫帶貫穿眼部，第二條則橫越鰓蓋後緣。鰓蓋後緣的上方另具一黑斑。各鰭皆為黃色，尾鰭另具黑色緣。大型魚之體側另散佈黑點。","strThumbnailUrl":null},{"strDataType":"video","strName":"無齒鯵","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2118/5bfa17f999639f6543a9bab02582720c.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2118/preview.jpg"},{"strDataType":"image","strName":"無齒鰺 ","strValue":"http://image.nmmba.gov.tw/upload/archive/3841/preview_20150914115058.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/3841/thumbnail_20150914115058.jpg"},{"strDataType":"image","strName":"海生館大洋池生物","strValue":"http://image.nmmba.gov.tw/upload/archive/7653/preview_20081229153418.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/7653/thumbnail_20081229153418.jpg"},{"strDataType":"image","strName":"無齒鰺","strValue":"http://image.nmmba.gov.tw/upload/archive/11490/preview_20110613091516.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/11490/thumbnail_20110613091516.jpg"},{"strDataType":"image","strName":"1272.\t無齒鰺 Gnathanodon speciosus (Forsskål, 1775)","strValue":"http://image.nmmba.gov.tw/upload/archive/21863/preview_20160715134335.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/21863/thumbnail_20160715134335.jpg"},{"strDataType":"image","strName":"無齒鰺","strValue":"http://image.nmmba.gov.tw/upload/archive/23449/preview_20161124102500.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23449/thumbnail_20161124102500.jpg"},{"strDataType":"image","strName":"無齒鰺","strValue":"http://image.nmmba.gov.tw/upload/archive/23450/preview_20161124102505.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23450/thumbnail_20161124102505.jpg"},{"strDataType":"image","strName":"無齒鰺","strValue":"http://image.nmmba.gov.tw/upload/archive/23711/preview_20161212104413.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23711/thumbnail_20161212104413.jpg"},{"strDataType":"image","strName":"無齒鰺","strValue":"http://image.nmmba.gov.tw/upload/archive/23993/preview_20161214105856.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23993/thumbnail_20161214105856.jpg"},{"strDataType":"image","strName":"","strValue":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/Gnathanodon speciosus1.jpg","strThumbnailUrl":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/Gnathanodon speciosus1.jpg"},{"strDataType":"image","strName":"","strValue":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/Gnathanodon speciosus2.jpg","strThumbnailUrl":"http://db.nmmba.gov.tw/BioSDS_WEB/BioSDS_Photo/Description/Fish/Gnathanodon speciosus2.jpg"}]