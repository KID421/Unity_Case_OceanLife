[{"strDataType":"text","strName":"中文名","strValue":"黃鰭短鬚石首魚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Umbrina roncador","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"石首魚科 (Sciaenidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"56 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於淺水域的沙地或潮池及潮間帶。食性以甲殼類、雙殼類、多毛類為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈紡錘狀，側扁，頭低且寬。口部水平，下顎較上顎短，下顎前緣下端具短粗硬的觸鬚，觸鬚兩旁則有一對黏液孔。外排齒列較大。鰓耙數15-22，背鰭根部陷於肉中並帶有鱗鞘，背鰭硬棘X-XI，軟條24-30，臀鰭硬棘II，軟條7。身體及頭部的鱗片較粗糙，靠近鰭的鱗片較為細緻，側線51-54。體色為帶有金屬光澤的藍色或灰色，背部略帶黃銅色，腹部銀白色，全身帶有橫向的黑色波浪紋，背鰭為黑色，其他魚鰭為黃色。","strThumbnailUrl":null},{"strDataType":"video","strName":"黃鰭短鬚石首魚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2186/c9d9c6881d12cedd55997c688792a57e.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2186/preview.jpg"},{"strDataType":"image","strName":"黃鰭短鬚石首魚","strValue":"http://image.nmmba.gov.tw/upload/archive/23723/preview_20161212112224.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23723/thumbnail_20161212112224.jpg"},{"strDataType":"image","strName":"黃鰭短鬚石首魚","strValue":"http://image.nmmba.gov.tw/upload/archive/23724/preview_20161212112237.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23724/thumbnail_20161212112237.jpg"},{"strDataType":"image","strName":"黃鰭短鬚石首魚","strValue":"http://image.nmmba.gov.tw/upload/archive/24000/preview_20161214113121.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24000/thumbnail_20161214113121.jpg"}]