[{"strDataType":"text","strName":"中文名","strValue":"紅尾高歡雀鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Hypsypops rubicundus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"雀鯛科 (Pomacentridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"30 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於具有岩石基質的清澈水域，常隱身於岩縫或石洞中，偶爾會在海草林中發現。具領域性。主要食性以無脊椎動物為主。其卵為沉性卵，雌魚會使魚卵黏著於基質上。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體高呈橢圓形，側扁，成熟的個體在前額具有肉瘤狀突起。鰓蓋邊緣平滑，沒有鱗片在下緣處，嘴巴小，可伸縮。單排齒呈現鑿子狀，單一連續背鰭，背鰭硬棘XII，軟條16-17，臀鰭硬棘II，軟條13-14。尾鰭上下兩端沒有較長的硬棘。成魚顏色為橙色，幼魚顏色較為鮮紅且帶有金屬藍色的斑點，尾鰭也會隨著成長逐漸從半透明轉為橙色。","strThumbnailUrl":null},{"strDataType":"video","strName":"紅尾高歡雀鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2062/077dd0107298a879ccee3bb36436def2.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2062/preview.jpg"},{"strDataType":"image","strName":"紅尾高歡雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23964/preview_20161214102311.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23964/thumbnail_20161214102311.jpg"},{"strDataType":"image","strName":"紅尾高歡雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23965/preview_20161214102319.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23965/thumbnail_20161214102319.jpg"}]