[{"strDataType":"text","strName":"中文名","strValue":"聯合真葉珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Euphyllia cristata","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"葵珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"聯合真葉珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Grape coral, Grape cristata torch coral","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"南部、澎湖、蘭嶼、綠島、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"生長於珊瑚礁斜坡下段或較深的礁石平臺上。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"群體由柵狀的分支構成，通常呈現半球型，每一分枝有1-3隻珊瑚蟲，珊瑚蟲日夜都會伸展，觸手成細長柔軟的管狀，觸手末端有球狀膨大，蟲體多呈綠色，膨大處多為淡橘色、黃色、白色或米色。","strThumbnailUrl":null},{"strDataType":"video","strName":"紅毛猩猩蟹 / 聯合真葉珊瑚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/286/8373c46fda0830a42b79b210e5290718.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/286/preview.jpg"},{"strDataType":"video","strName":"日本英雄蟹 / 聯合真葉珊瑚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/757/b790925081ef0444832dd2387cb88b65.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/757/preview.jpg"},{"strDataType":"video","strName":"日本英雄蟹 / 聯合真葉珊瑚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1399/08e87cae3ce2203e05f47a2857aa4d66.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1399/preview.jpg"},{"strDataType":"image","strName":"聯合真葉珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/6116/preview_006116.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/6116/thumbnail_006116.jpg"},{"strDataType":"image","strName":"聯合真葉珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/16719/preview_20130730084430.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/uplo