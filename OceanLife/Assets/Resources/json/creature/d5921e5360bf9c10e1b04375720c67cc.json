[{"strDataType":"text","strName":"中文名","strValue":"太平洋竹筴魚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Trachurus symmetricus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鰺科 (Carangidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"81 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"成體棲息離岸較遠的海域，並且會形成大群體群游，幼魚則成群棲息於海藻林區。食性主要以小型甲殼類或小魚為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈圓柱形，細長，上下顎各單排牙齒，齒小，具有脂肪眼瞼，背鰭硬棘VII-VIII，軟條28-38，臀鰭硬棘I-II，軟條22-23，脊椎骨數22-23。","strThumbnailUrl":null},{"strDataType":"video","strName":"太平洋竹筴魚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2180/f8fd073aaa2bd2f92d33cf42ff829472.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2180/preview.jpg"},{"strDataType":"image","strName":"太平洋竹筴魚","strValue":"http://image.nmmba.gov.tw/upload/archive/23897/preview_20161213164948.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23897/thumbnail_20161213164948.jpg"},{"strDataType":"image","strName":"太平洋竹筴魚","strValue":"http://image.nmmba.gov.tw/upload/archive/23898/preview_20161213164958.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23898/thumbnail_20161213164958.jpg"}]