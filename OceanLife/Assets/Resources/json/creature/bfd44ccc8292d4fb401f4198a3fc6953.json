[{"strDataType":"text","strName":"中文名","strValue":"南方海棕梠","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Eisenia arborea","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"巨藻科 (Lessoniaceae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"南方海棕梠","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Southern sea palm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"北太平洋、東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"通常分布於潮間帶至亞潮帶，可食用，具有藥用功能，為許多生物的營養來源。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"根系附著於堅硬基質上，根系連接一根著堅韌且細長的莖狀結構，頂端再延伸出兩分枝，分枝上生長數片葉片。隨著莖狀結構成長，莖狀結構半徑及質量會與之增加，此外葉片大小及質量也會增加。葉片型態會受到海水流速的影響，流速較低的地區，葉片較寬，葉片表面的突起也較大。在流速較高的地區，葉片則較長且較細。","strThumbnailUrl":null},{"strDataType":"video","strName":"海棕梠","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2244/2b1b720b0f6e5bb5fffc044578b9b0aa.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2244/preview.jpg"},{"strDataType":"image","strName":"南方海棕梠","strValue":"http://image.nmmba.gov.tw/upload/archive/23709/preview_20161212104348.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23709/thumbnail_20161212104348.jpg"}]