[{"strDataType":"text","strName":"中文名","strValue":"白鯨","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Delphinapterus leucas","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"一角鯨科 (Monodontidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"貝魯卡鯨","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Beluga whale, sea canary","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"550 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"北極海","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於北極海域，多出現在靠近海岸的淺水域，也常在鄰近的峽灣、河口、海灣出現。也曾有在較深水域發現此物種的蹤跡。受到夏季融冰的影響，或者隨著洄游性魚類移動，具有隨季節遷移的習性。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體長渾圓，頭尾較窄，相較一般鯨類在於有明顯且靈活的頸部關節可以轉動。體色純白或灰色，幼體出生時為灰色，在1個月大時體色會轉為深灰色或藍灰色，隨色素消退漸轉為白色。胸鰭及尾鰭會隨著年齡增長有逐漸變方的趨勢。","strThumbnailUrl":null},{"strDataType":"video","strName":"1050105_00017","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1563/383017aca3ec13c025df61b59f01f229.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1563/preview.jpg"},{"strDataType":"video","strName":"1050105_00012","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1568/81d81453dafef811044e978225df2b87.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1568/preview.jpg"},{"strDataType":"video","strName":"1050105_00001","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1571/ca70f782726ca94a579fd0283dcae0c1.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1571/preview.jpg"},{"strDataType":"video","strName":"1050105_00005","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1572/dc630c4e14af334b9b902841f5146ece.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1572/preview.jpg"},{"strDataType":"image","strName":"白鯨","strValue":"http://image.nmmba.gov.tw/u