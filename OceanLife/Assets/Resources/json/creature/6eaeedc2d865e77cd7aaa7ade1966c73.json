[{"strDataType":"text","strName":"中文名","strValue":"美麗硬骨舌魚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Scleropages formosus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"骨舌魚科 (Osteoglossidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"80 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"緬甸、馬來半島、印尼、泰國","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於混濁且具腐植質或泥炭基底的深色淡水域，該水域大多為森林中河流或湖泊。食性以水表的昆蟲或小魚，甚至小型的哺乳類為食。具有口孵幼魚的習性。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體延長，側扁，頭部及口部大且開口傾斜，下顎吻端具兩短觸鬚，鰓耙粗壯，口內許多部位骨骼皆有小齒。具有大片的圓鱗，眼大，胸鰭長，尾鰭圓型，背鰭及臀鰭結束位置接近尾鰭起點。具有多種顏色，且具有金屬光澤。","strThumbnailUrl":null},{"strDataType":"video","strName":"美麗硬骨舌魚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1943/e07c2367869e0f2407736f378117fbf7.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1943/preview.jpg"},{"strDataType":"image","strName":"紅龍","strValue":"http://image.nmmba.gov.tw/upload/archive/20311/preview_20151028120101.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/20311/thumbnail_20151028120101.jpg"},{"strDataType":"image","strName":"紅龍","strValue":"http://image.nmmba.gov.tw/upload/archive/20320/preview_20151029153614.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/20320/thumbnail_20151029153614.jpg"}]