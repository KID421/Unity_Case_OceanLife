[{"strDataType":"text","strName":"中文名","strValue":"副刻齒雀鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Chrysiptera parasema","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"雀鯛科","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"黃尾藍魔、黃尾雀","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Goldtail demoiselle, Yellow tail damselfish","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"7 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"西太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於沿海礁區，棲息深度介於1-16公尺深。成體棲息於潟湖或近岸珊瑚礁，通常小群體聚集在分支型珊瑚之間，繁殖期間，雌雄各自成對，雌魚會將卵產至堅硬的基質，雄魚則有護卵行為如以煽動魚卵確保水流交換和抵禦外敵。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"頭和身體呈現藍色在接近尾部末段轉為黃色，體呈橢圓而側扁，背鰭硬棘XIII，軟條10-12，臀鰭硬棘II，軟條11-12","strThumbnailUrl":null},{"strDataType":"video","strName":"副刻齒雀鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2031/79bff1c9fd57877def4e03b023f6d3d0.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2031/preview.jpg"},{"strDataType":"image","strName":"副刻齒雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23715/preview_20161212111652.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23715/thumbnail_20161212111652.jpg"},{"strDataType":"image","strName":"副刻齒雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23716/preview_20161212111702.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23716/thumbnail_20161212111702.jpg"},{"strDataType":"image","strName":"副刻齒雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23717/preview_20161212111711.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23717/thumbnail_20161212111711.jpg"},{"strDataType":"image","strName":"副刻齒雀鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23718/preview_20161212111719.jpg","strThumbnailUrl":"http: