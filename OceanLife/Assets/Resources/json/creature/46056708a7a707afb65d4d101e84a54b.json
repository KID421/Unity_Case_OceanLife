[{"strDataType":"text","strName":"中文名","strValue":"疣表孔珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Montipora verrucosa","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"軸孔珊瑚科","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"疣表孔珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Scroll coral","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、南部、澎湖、小琉球、綠島、蘭嶼、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"通常生長在海流適中的珊瑚礁平臺或斜坡上段。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"珊瑚體呈葉片狀伸展，表面密布疣狀突起。突起的大小和形狀都很一致，約為3公釐，分布相當均勻。珊瑚孔為凹入型，均勻散佈在疣狀突起之間，孔口直徑約0.9-1.3公釐，隔片兩環，具成列的細棘。生活群體呈黃色、黃褐色、或綠褐色。雌雄同體、排放型。","strThumbnailUrl":null},{"strDataType":"image","strName":"表孔珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/3573/preview_003573.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/3573/thumbnail_003573.jpg"}]