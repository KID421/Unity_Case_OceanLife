[{"strDataType":"text","strName":"中文名","strValue":"線紋刺尾鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Acanthurus lineatus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"刺尾鯛科 (Acanthuridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"紋倒吊、彩虹倒吊","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"38 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、南部、北部、東北部、澎湖、小琉球、蘭嶼、綠島、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於珊瑚礁或岩礁之浪拂區，棲息深度在0-15公尺左右。具有強烈之領域性，由1尾較大雄魚控制界限分明的攝食領地及一群雌魚。本種魚通常會不停的移動。草食性，但也捕食甲殼類。尾柄上的直立前向棘具有毒腺，能使人引起劇痛。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈橢圓形而側扁。頭背部輪廓不特別凸出。口小，端位，上下頜各具一列扁平齒，齒固定不可動，齒緣具缺刻。背鰭及臀鰭硬棘尖銳，分別具XI棘及III棘，各鰭條皆不延長；尾鰭彎月形，隨著成長，上下葉逐漸延長。尾柄棘尖銳而極長。頭部及體側上部約3/4的部位為黃色，並具有8-11條鑲黑邊的藍色縱紋，上部數條伸達背鰭；下部則為淡藍色。腹鰭橘黃至鮮橘色且具黑緣；尾鰭前部暗褐色，後接1條藍色彎月紋，彎月紋後有一片淡藍色區，上下葉為黃褐色；餘鰭淡褐色至黃褐色；奇鰭皆具藍色緣。","strThumbnailUrl":null},{"strDataType":"video","strName":"線紋刺尾鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/4/5c48c43741f2c0b1b8a4a3366793d2ef.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/4/preview.jpg"},{"strDataType":"image","strName":"線紋刺尾鯛 ","strValue":"http://image.nmmba.gov.tw/upload/archive/3529/preview_20121121091945.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/3529/thumbnail_20121121091945.jpg"},{"strDataType":"image","strName":"線紋刺尾鯛 ","strValue":"http://image.nmmba.gov.tw/upload/archive/4042/preview_20100708164656.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4042/thumbnail_20100708164656.jpg"},{"strDataType":"image","strName":"線紋刺尾鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/4190/preview_004190.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4190/thumbnail_004190.jpg