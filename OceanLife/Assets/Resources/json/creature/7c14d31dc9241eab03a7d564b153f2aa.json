[{"strDataType":"text","strName":"中文名","strValue":"牡丹梳珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Pectinia paeonia","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"梳狀珊瑚科 (Pectiniidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"梳珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Spiny cup pectinia coral","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東北部、東部、南部、澎湖、小琉球、綠島、蘭嶼、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"通常分布於珊瑚礁礁斜坡或潟湖的隱蔽處，偏好濁度較高的水域，分布深度介於5-20公尺。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"珊瑚群體形狀特殊，整體由珊瑚孔延長、直立且螺旋而成的肋片集結包覆形成，隔片表面平滑，中柱不發達。珊瑚群體顏色呈灰色、綠色或褐色，偶爾會呈斑駁的色澤。","strThumbnailUrl":null},{"strDataType":"video","strName":"牡丹梳珊瑚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2079/91a1a5af8c1ac9ab8bec4fe25f96c260.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2079/preview.jpg"},{"strDataType":"image","strName":"牡丹梳珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/8235/preview_20090910135608.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/8235/thumbnail_20090910135608.jpg"},{"strDataType":"image","strName":"牡丹梳珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/8676/preview_20100407105803.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/8676/thumbnail_20100407105803.jpg"},{"strDataType":"image","strName":"牡丹梳珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/10889/preview_20110411085551.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/10889/thumbnail_20110411085551.jpg"},{"strDataType":"image","strName":"牡丹梳珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/12165/preview_20111031080619.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/