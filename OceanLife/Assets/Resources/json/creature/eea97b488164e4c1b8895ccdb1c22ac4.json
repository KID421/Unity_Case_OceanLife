[{"strDataType":"text","strName":"中文名","strValue":"考氏鰭竺鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Pterapogon kauderni","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"天竺鯛科 (Apogonidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"泗水玫瑰","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Banggai cardinal fish","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"8 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"Banggai 群島","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於砂質或海草床的淺水海灣或港口，可在水深0.5公尺處即發現，多棲息於1.5至2.5公尺深的水層，最深可達6公尺。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈橢圓形，稍側扁，背鰭硬棘VIII，軟條14；臀鰭硬棘II，軟條13。具有流蘇狀的第一背鰭，第二背鰭和臀鰭也皆有延長的鰭膜。體色分布主要帶有三條黑色縱帶分別自頭部至喉部、第一背鰭至腹鰭、第二背鰭至臀鰭。分岔的尾鰭邊緣也各帶有兩條黑色橫帶，除了尾鰭，第二背鰭和臀鰭邊緣皆帶有規則排列的白點，在兩背鰭之前的區域則帶有銀白色的小圓點，此白點可用以辨識不同的個體。","strThumbnailUrl":null},{"strDataType":"video","strName":"考氏鰭天竺鯛","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2030/54392230b96ee4df2bb55962ed781df6.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2030/preview.jpg"},{"strDataType":"image","strName":"考氏鰭竺鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23414/preview_20161124094748.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23414/thumbnail_20161124094748.jpg"},{"strDataType":"image","strName":"考氏鰭竺鯛","strValue":"http://image.nmmba.gov.tw/upload/archive/23415/preview_20161124094846.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23415/thumbnail_20161124094846.jpg"}]