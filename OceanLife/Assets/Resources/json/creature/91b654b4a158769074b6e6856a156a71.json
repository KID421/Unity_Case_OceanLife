[{"strDataType":"text","strName":"中文名","strValue":"大口副鱸","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Paralabrax clathratus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鮨科 (Serranidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"72 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於海藻床等淺水域，最深可達50公尺深，但多分布於2.4公尺至21公尺處。幼魚以底棲無脊椎生物為食，偏好甲殼類，成魚則以其他魚類以及頭足類為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈紡錘狀，側扁。體高中等，頭尖，口大，前鰓蓋具有鋸齒，雙眼間距短，頭部具有鱗片，背鰭硬棘X，軟條3-14，第三和第四硬棘等長，但不比第二硬棘兩倍長。臀鰭硬棘III，軟條7；胸鰭長圓；尾鰭截型。","strThumbnailUrl":null},{"strDataType":"video","strName":"大口副鱸","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2088/234cb828699af8529591c494a9e4f8c5.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2088/preview.jpg"},{"strDataType":"image","strName":"大口副鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23890/preview_20161213163105.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23890/thumbnail_20161213163105.jpg"},{"strDataType":"image","strName":"大口副鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23891/preview_20161213163118.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23891/thumbnail_20161213163118.jpg"},{"strDataType":"image","strName":"大口副鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23966/preview_20161214102327.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23966/thumbnail_20161214102327.jpg"},{"strDataType":"image","strName":"大口副鱸","strValue":"http://image.nmmba.gov.tw/upload/archive/23967/preview_20161214102335.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23967/thumbnail_20161214102335.jpg"}]