[{"strDataType":"text","strName":"中文名","strValue":"紅滑皮海星","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Leiaster speciosus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"蛇星科 (Ophidiasteridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東北部、澎湖、蘭嶼","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至西太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"以底棲無脊椎生物為食，多分布於淺海域。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"具有5個較為細長且呈指狀的腕，末稍翹起，間輻區域小，全身體表覆蓋著厚且平滑的表皮，骨板呈網狀排列，呈9縱列，並有呈8縱列的皮鰓孔成群座落在骨板間，骨板上覆蓋著平滑的厚皮膜。次步帶棘為匙狀，體盤中央有單一肛門，周圍有7個鈣質齒圍繞。具有單一呈凹面的篩板，上面有許多放射狀紋路，皮鰓區及步帶板上有稀疏的白色瓣狀叉棘，溝棘兩個一組，上有皮膜相連，亞溝棘1個，且異常膨大，末端呈漿狀。此物種的篩板大，生活時體色為鮮紅色，乾製標本的體色則呈淡棕色，篩板凹陷，上有許多放射狀的細溝紋。","strThumbnailUrl":null},{"strDataType":"video","strName":"紅滑皮海星","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2243/4acb1bb66118c6480c904f578cdcf9dc.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2243/preview.jpg"}]