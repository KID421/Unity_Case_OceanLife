[{"strDataType":"text","strName":"中文名","strValue":"羽蛇藻","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Egregia menziesii","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"翅藻科 (Alariaceae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"羽蛇藻","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Feather boa kelp","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"西太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"分布於潮間帶的常見海藻","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"根系附著於堅硬基質上，根系連結樹根堅韌的莖狀結構，莖狀結構的周圍分布葉片，密集且排列整齊，葉片狀長、扁平、略呈帶狀，並且在較近莖狀結構處又分布較小的葉片。具氣囊狀葉狀體。個體差異大，莖狀結構呈光滑表面或者帶有波紋，葉片邊緣也具多種形狀。","strThumbnailUrl":null},{"strDataType":"video","strName":"羽蛇藻","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2241/8afff97e01a45c6b26f974d14dc9eb80.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2241/preview.jpg"},{"strDataType":"image","strName":"羽蛇藻","strValue":"http://image.nmmba.gov.tw/upload/archive/23692/preview_20161212101832.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23692/thumbnail_20161212101832.jpg"}]