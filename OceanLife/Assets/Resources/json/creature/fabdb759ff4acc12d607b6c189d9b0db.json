[{"strDataType":"text","strName":"中文名","strValue":"緣鰭海豬魚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Halichoeres marginatus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"隆頭魚科 (Labridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Two-eyed wrasse, Splendid rainbow wrasse, Dusky wrasse","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"18 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、西部、南部、北部、東北部、澎湖、小琉球、蘭嶼、綠島、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋  ","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於潟湖或珊瑚礁向海面；幼魚較常出現在礁岩平臺的外圍，成魚則集中在珊瑚礁生長繁茂地方。以無脊椎動物，如甲殼類、軟體類、多毛蟲、有孔蟲以及魚卵等為食 ","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體延長，側扁。吻較長，尖突。前鼻孔具短管。口小；上頜有犬齒四枚，外側兩枚向後方彎曲。除枕區外頭部全裸；背鰭與臀鰭有低鱗鞘。D. IX, 13-14；A. III, 12-13；P. 14-15；L.l. 27-28；G.R. 17-20；後期雄魚腹鰭長達臀鰭起點之後。幼魚體黑色，具多條不連續的淡黄色細縱線，背鰭臀鰭黑色，背鰭前端具一鑲黃邊的小黑眼斑，中央則具一鑲黃邊的大黑眼斑，尾鰭白色；雌魚黑褐色，頭部佈滿淡色紋，體側各鱗片具一暗點，而形成暗點列，餘同幼魚；雄魚暗綠色至橘褐色，頭部佈滿暗色紋，各鱗片具一紫藍色點，而形成暗點列，與頭部的縱線相連，尾鰭前端具一新月形的藍緣黄綠色橫帶，後接一新月形橙色寬帶，橙色寬帶內散佈暗緣之藍緣斑點，再接藍色新月形窄帶，末緣則為黄色。","strThumbnailUrl":null},{"strDataType":"video","strName":"緣鰭海豬魚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1811/902aab3121b6b3a2c9087716c43457a2.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1811/preview.jpg"},{"strDataType":"image","strName":"緣鰭海豬魚","strValue":"http://image.nmmba.gov.tw/upload/archive/5291/preview_005291.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/5291/thumbnail_005291.jpg"},{"strDataType":"image","strName":"綠鰭海豬魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/9179/preview_20100621135044.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/9179/thumbnail_20100621135044.jpg"},{"strDataType":"image","strName":"綠鰭海豬魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/10334/preview_20121003162933.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/10334/thumbnail_20121003162933.jpg"},{"strDataType":"image","strName":"綠鰭海豬魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/10476/preview_20101215111511.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/10476/thumbnail_20101215111511.jpg"},{"strDataType":"image","strName":"綠鰭海豬魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/10477/preview_20121217115730.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/10477/thumbnail_20121217115730.jpg"},{"strDataType":"image","strName":"綠鰭海豬魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/10605/preview_20130814161933.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/10605/thumbnail_20130814161933.jpg"},{"strDataType":"image","strName":"綠鰭海豬魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/15233/preview_20121217093022.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/15233/thumbnail_20121217093022.jpg"},{"strDataType":"image","strName":"1773.\t緣鰭海豬魚 Halichoeres marginatus Rüppell, 1835","strValue":"http://image.nmmba.gov.tw/upload/archive/22849/preview_20160919090604.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22849/thumbnail_20160919090604.jpg"}]