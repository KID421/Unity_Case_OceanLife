[{"strDataType":"text","strName":"中文名","strValue":"分離指形軟珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Sinularia exilis","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、西部、南部、北部、東北部、澎湖、小琉球、蘭嶼、綠島、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"越南、臺灣、琉球群島","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"通常分布於海流較強且容易受到海浪影響的珊瑚礁表面，分布深度介於3-20公尺，最常見於5-15公尺處。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"珊瑚群體為表覆形，肉質表面具有許多高度不等的指狀突起。不具完整骨骼，肉質組織以骨針支撐，表層骨針呈棒形或紡錘形，長度分別介於0.08-0.12公釐以及0.14-0.23公釐。內部骨針則較為粗大，呈紡錘形，長度可達4公釐。珊瑚蟲為絨毛狀。","strThumbnailUrl":null},{"strDataType":"video","strName":"分離指形軟珊瑚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2083/c6e3016141b7ce45b4f47abbcd0a8ca1.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2083/preview.jpg"},{"strDataType":"video","strName":"分離指形軟珊瑚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2157/388aa3c1e86927bb292c68e34907220e.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2157/preview.jpg"}]