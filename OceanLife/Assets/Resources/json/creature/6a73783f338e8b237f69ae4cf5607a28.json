[{"strDataType":"text","strName":"中文名","strValue":"藍平鮋","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Sebastes mystinus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鮋科 (Scorpaenidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"61 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於礁岩地區的淺水域，也常出現在海藻林四周以及離岸較遠的深水礁區。幼魚則是遠洋性，會群游且和其他同屬的幼魚群游，幼魚階段食性主要以磷蝦為主，成魚則是以被囊生物以及水母等生物為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈紡錘狀，背鰭硬棘XIII，軟條15-17，臀鰭硬棘III，軟條8-10，脊椎骨數26-27，頭部強棘較小，眼距短，口小，鰓蓋條7，體色多為藍色或黑色，腹部顏色較淺，額頭有模糊的條紋。","strThumbnailUrl":null},{"strDataType":"video","strName":"藍平鮋","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2073/63e785b9ce1b77085fa55fb344687663.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2073/preview.jpg"},{"strDataType":"video","strName":"藍平鮋","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2263/6980888f3facc1cdafebffb4597cf5f2.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2263/preview.jpg"},{"strDataType":"image","strName":"藍平鮋","strValue":"http://image.nmmba.gov.tw/upload/archive/23731/preview_20161212143807.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23731/thumbnail_20161212143807.jpg"},{"strDataType":"image","strName":"藍平鮋","strValue":"http://image.nmmba.gov.tw/upload/archive/23732/preview_20161212143821.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23732/thumbnail_20161212143821.jpg"},{"strDataType":"image","strName":"藍平鮋","strValue":"http://image.nmmba.gov.tw/upload/archive/24073/preview_20161220085325.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24073/thumbnail_20161220085325.jpg"}]