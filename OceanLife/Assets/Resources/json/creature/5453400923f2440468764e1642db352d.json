[{"strDataType":"text","strName":"中文名","strValue":"吉打龍紋鱝","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Rhynchobatus djiddensis","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"龍紋鱝科 (Rhynchobatidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"飯匙鯊、魴仔","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"300 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、西部、南部、西南部、澎湖","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"西太平洋 ","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"近沿海底棲性魚種，從浪拂區到50公尺，活動力不強，以沙泥底之蟹、龍蝦、貝、魚類為食。 ","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體型呈三角形，長度大於寬度，口裂中央彎曲，齒大，平滑呈菱形，噴水孔位於眼之近後方，尾部與軀幹部不易區分，外形介於鮫類與鱝類之間，第一背鰭起點與腹鰭起點同，尾鰭後緣凹入，分為上下兩葉，沿中央脊柱之瘤狀突起分布較短而不達尾端。體呈黃褐色，具白點於背部、腹側淡灰；胸鰭基部的眼點明顯。","strThumbnailUrl":null},{"strDataType":"video","strName":"吉打龍紋鱝","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2150/941f5103c4d8ba84c078b650598171c5.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2150/preview.jpg"},{"strDataType":"image","strName":"126.\t吉打龍文鱝 Rhynchobatus djiddensis (Forsskål, 1775)","strValue":"http://image.nmmba.gov.tw/upload/archive/20621/preview_20160217160545.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/20621/thumbnail_20160217160545.jpg"},{"strDataType":"image","strName":"吉打龍紋鱝","strValue":"http://image.nmmba.gov.tw/upload/archive/23687/preview_20161212101125.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23687/thumbnail_20161212101125.jpg"},{"strDataType":"image","strName":"吉打龍紋鱝","strValue":"http://image.nmmba.gov.tw/upload/archive/23688/preview_20161212101141.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23688/thumbnail_20161212101141.jpg"}]