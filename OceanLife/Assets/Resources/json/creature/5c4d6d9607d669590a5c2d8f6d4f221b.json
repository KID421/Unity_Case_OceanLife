[{"strDataType":"text","strName":"中文名","strValue":"汙木葉鰈","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Pleuronichthys coenosus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鰈科 (Pleuronectidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"36 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於海岸斜坡下或礁岩海域，在夏季，幼魚時常出現於淺水區域，成魚則普遍棲息於較深的海域。食性以底棲多毛類、雙殼類及端足類為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈盤狀扁平，兩眼皆位於右邊，兩顆球狀的眼睛接近口端。胸鰭無硬棘，軟條65-78，臀鰭無硬棘，軟條46-56，脊椎數36-38。背鰭生長點位於覆蓋面，尾鰭寬圓，胸鰭長度適中末端較尖。體表顏色多為棕色帶有白色斑點。","strThumbnailUrl":null},{"strDataType":"video","strName":"汙木葉鰈","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2054/458217a3a9721b2e3897393d64cc866a.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2054/preview.jpg"},{"strDataType":"image","strName":"汙木葉鰈","strValue":"http://image.nmmba.gov.tw/upload/archive/23689/preview_20161212101142.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23689/thumbnail_20161212101142.jpg"},{"strDataType":"image","strName":"汙木葉鰈","strValue":"http://image.nmmba.gov.tw/upload/archive/23690/preview_20161212101155.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23690/thumbnail_20161212101155.jpg"}]