[{"strDataType":"text","strName":"中文名","strValue":"皮柳珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Briareum violacea","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"皮軟珊瑚科","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"紫皮珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Star polyp, Violet skin coral","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、南部、東北部、澎湖、小琉球、蘭嶼、綠島、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"生長於水體較混濁的淺海礁岩上。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"山 胡崇單體高約1.5公分，可收入組織內，中央口部明顯且突出，觸手薄而略扁，邊緣羽化不明顯或退化。骨針多而大型，多為紡錘狀或有分支，表面的突起成結節狀，長度介於0.1-0.5公釐。顏色有白色至深紅色。珊瑚群體呈薄皮狀，表面呈淡褐色或紅褐色，具共生藻。","strThumbnailUrl":null},{"strDataType":"image","strName":"紫皮珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/8426/preview_20100201140502.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/8426/thumbnail_20100201140502.jpg"},{"strDataType":"image","strName":"紫皮珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/9859/preview_20101012115335.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/9859/thumbnail_20101012115335.jpg"},{"strDataType":"image","strName":"紫皮珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/10757/preview_20110314095009.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/10757/thumbnail_20110314095009.jpg"},{"strDataType":"image","strName":"紫皮珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/10893/preview_20110411085720.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/10893/thumbnail_20110411085720.jpg"},{"strDataType":"image","strName":"紫皮珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/12132/preview_20111024155625.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/arch