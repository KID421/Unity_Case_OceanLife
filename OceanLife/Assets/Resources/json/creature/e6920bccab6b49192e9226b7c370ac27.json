[{"strDataType":"text","strName":"中文名","strValue":"條紋活額蝦","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Rhynchocinetes striatus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"活額蝦科 (Rhynchocinetidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"南部","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於淺水區域的礁岩之間。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"具有較大的黑色複眼，各段體節略帶透明並佈滿紅色以及白色線條及斑點，在頭胸部具有一側躺的Y字形為其特徵之一。雄性個體的第一步足特化為大螯。為Rhynchocinetes durbanensis的同種異名。","strThumbnailUrl":null},{"strDataType":"image","strName":"條紋活額蝦","strValue":"http://image.nmmba.gov.tw/upload/archive/13838/preview_20120515090927.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/13838/thumbnail_20120515090927.jpg"},{"strDataType":"image","strName":"條紋活額蝦","strValue":"http://image.nmmba.gov.tw/upload/archive/13839/preview_20120515090942.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/13839/thumbnail_20120515090942.jpg"}]