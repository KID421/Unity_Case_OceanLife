[{"strDataType":"text","strName":"中文名","strValue":"灌叢柳珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Rumphella antipathes","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"柳珊瑚科 (Gorgoniidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"灌叢柳珊瑚","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Rumphella gorgonia","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"南部、綠島、蘭嶼、東沙、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於水深介於10-25公尺處的各種珊瑚礁環境，期共嗽組織含有抗菌化學物質。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"珊瑚體呈樹叢形，高度可達1公尺以上，群體基部有鈣化的附著器黏附於在礁石上，其上的主幹短，分支多為細長的鞭狀。共肉組織甚厚，中央為角質的柱形中軸骨。珊瑚蟲單型，均勻分布在分支表面，可收縮素共肉組織工。表層骨針為棒形，表面多突起；珊瑚蟲內的骨針為紡錘形。生活群體呈灰綠色或淡藍色，珊瑚蟲為褐色或黃色。","strThumbnailUrl":null},{"strDataType":"video","strName":"灌叢柳珊瑚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2089/f70b1620bac8d67f7d7943609b54087c.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2089/preview.jpg"},{"strDataType":"image","strName":"叢柳珊瑚","strValue":"http://image.nmmba.gov.tw/upload/archive/334/preview_000334.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/334/thumbnail_000334.jpg"}]