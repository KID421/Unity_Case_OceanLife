[{"strDataType":"text","strName":"中文名","strValue":"藍豬齒魚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Choerodon azurio","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"隆頭魚科 (Labridae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"四齒仔、西齒、寒鯛","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"40 cm ","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、西部、南部、北部、東北部、澎湖、小琉球、蘭嶼、綠島、南沙","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"日本至南中國海","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於岩岸礁區，水深約7-80公尺的海域。以底棲性生物為主食，由於有著二對尖銳犬齒，所以可以輕易咬碎厚厚外殼的貝類及甲殼類等。它們白天覓食，夜晚藏身於隱密的岩蔭或岩穴之中。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體延長而呈長卵圓形，頭部背面輪廓圓凸，頭前端與吻部成一大傾斜角度。上下頜突出，前端具 4大犬齒，上頜兩側具擴大犬齒。D. XIII,8；A. III,11；P. 16；L.l. 27-29；l.tr. 3-4/9-10；背鰭連續；尾鰭稍圓形。側線連續，呈圓弧狀；頰部與鰓蓋被鱗。體呈淺紅褐色，胸鰭上方有2條斜向背鰭基部的相鄰斜帶，其中前方一條顏色為黑至暗褐色，另一條則為白至粉紅色。幼魚全身都是紅褐色，斜帶乃隨成長而出現。","strThumbnailUrl":null},{"strDataType":"video","strName":"藍豬齒魚","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2126/9f31bfd395f24eb4acdb8df5f1d3ddf1.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2126/preview.jpg"},{"strDataType":"image","strName":"藍豬齒魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/3551/preview_20130305105152.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/3551/thumbnail_20130305105152.jpg"},{"strDataType":"image","strName":"藍豬齒魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/15587/preview_20130208103855.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/15587/thumbnail_20130208103855.jpg"},{"strDataType":"image","strName":"藍豬齒魚","strValue":"http://image.nmmba.gov.tw/upload/archive/15588/preview_20130208103909.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/15588/thumbnail_20130208103909.jpg"},{"strDataType":"image","strName":"藍豬齒魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/15696/preview_20130219085654.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/15696/thumbnail_20130219085654.jpg"},{"strDataType":"image","strName":"藍豬齒魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/15870/preview_20130415145358.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/15870/thumbnail_20130415145358.jpg"},{"strDataType":"image","strName":"藍豬齒魚 ","strValue":"http://image.nmmba.gov.tw/upload/archive/16638/preview_20130708142607.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/16638/thumbnail_20130708142607.jpg"},{"strDataType":"image","strName":"1747.\t藍豬齒魚 Choerodon azurio (Jordan & Snyder, 1901)","strValue":"http://image.nmmba.gov.tw/upload/archive/22823/preview_20160919090007.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22823/thumbnail_20160919090007.jpg"},{"strDataType":"image","strName":"藍豬齒魚","strValue":"http://image.nmmba.gov.tw/upload/archive/24076/preview_20161220085354.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/24076/thumbnail_20161220085354.jpg"}]