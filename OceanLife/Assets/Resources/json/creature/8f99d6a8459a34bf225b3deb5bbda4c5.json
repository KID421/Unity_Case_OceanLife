[{"strDataType":"text","strName":"中文名","strValue":"穗平鮋","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Sebastes auriculatus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鮋科 (Scorpaenidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"56 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於礁岩地區的淺水域，常趴在岩石上或石洞中發現，其最深層出現在水深135公尺。食性以其他魚類、甲殼類、頭足類為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈紡錘狀，頭大，眼前後部有凹陷，背鰭硬棘XIII，軟條12-15，臀鰭硬棘III，軟條5-8，脊椎骨數26-27，尾鰭圓形。體色多為淺棕色，背部顏色較深，鰓蓋上部有深棕色斑點，魚鰭和喉部以及下顎為粉紅色。","strThumbnailUrl":null},{"strDataType":"video","strName":"穗平鮋","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2187/5a70c61d29ad6bef810720d092b113ef.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2187/preview.jpg"},{"strDataType":"image","strName":"穗平鮋","strValue":"http://image.nmmba.gov.tw/upload/archive/23734/preview_20161212143843.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23734/thumbnail_20161212143843.jpg"}]