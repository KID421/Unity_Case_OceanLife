[{"strDataType":"text","strName":"中文名","strValue":"赤鯮","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Dentex tumifrons","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"鯛科 (Sparidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"赤章","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Red sea-bream, Yellow seabream, Yellowback seabream, Golden tai","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"30.7 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東北部","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"西太平洋  ","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於沿岸及大陸棚砂泥底質的水域，棲息深度在50-150公尺間，以底棲動物，如甲殼類、軟體動物及小魚等為主食。  ","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"背鰭單一，硬棘部及軟條部間無明顯缺刻，硬棘皆不延長如絲狀，硬棘數XII，軟條數10；臀鰭小，與背鰭軟條部同形，第II及第III棘約等長，軟條數8；胸鰭長，長於腹鰭；尾鰭叉形。體側呈鮮紅色而帶銀色光澤，愈近腹部逐漸變淡，腹部呈粉紅至白色；5-6條鈷藍色的點狀線紋分布於側線下方水平列的鱗片之間，以傾斜的角度觀看背側面可以看見側線上方具有藍色的點狀斜紋；體背側無任何大型黃色斑點，前鼻部與吻部之間亦無黃色污斑。背鰭、臀鰭及尾鰭橘紅色，鰭膜上有些黃色斑紋。","strThumbnailUrl":null},{"strDataType":"image","strName":"1453.\t赤鯮（黃牙鯛） Dentex tumifrons (Temminck & Schlegel, 1843)","strValue":"http://image.nmmba.gov.tw/upload/archive/22220/preview_20160815093106.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22220/thumbnail_20160815093106.jpg"}]