[{"strDataType":"text","strName":"中文名","strValue":"半帶皺唇鯊","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Triakis semifasciata","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"皺唇鯊科 (Triakidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"198 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於淺水域，常出現於封閉的海灣、潟湖或河口。食性廣泛，包含甲殼類、螺貝類、魚卵。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈雪茄形，吻端短圓，鼻孔前端有三角形的皮瓣，眼大，橢圓，具有瞬膜，口部唇線彎曲。上顎齒列齒數41-55，下顎齒列齒數34-45。第一背鰭位在胸鰭及腹鰭中間位置，第二背鰭與第一背鰭幾乎等大，但大於臀鰭。胸鰭狀三角形。尾鰭下部在成熟後才會發展完全，但是大小不到上部的一半。體色獨特，多呈銀色或青銅色，帶有黑色塊斑，背部具有多塊鞍帶塊斑。成魚的塊斑會較幼魚更多且塊斑中心顏色更淡。","strThumbnailUrl":null},{"strDataType":"video","strName":"半帶皺唇鯊","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/2051/7f8013c9def6d669e0e04356640ff34e.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/2051/preview.jpg"},{"strDataType":"image","strName":"半帶皺唇鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23237/preview_20161104134856.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23237/thumbnail_20161104134856.jpg"},{"strDataType":"image","strName":"半帶皺唇鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23918/preview_20161214084326.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23918/thumbnail_20161214084326.jpg"},{"strDataType":"image","strName":"半帶皺唇鯊","strValue":"http://image.nmmba.gov.tw/upload/archive/23919/preview_20161214084336.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/23919/thumbnail_20161214084336.jpg"}]