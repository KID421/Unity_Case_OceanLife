[{"strDataType":"text","strName":"中文名","strValue":"蝠海星","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Asterina miniata","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"海燕科 (Asterinidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"蝠海星","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Bat star","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"東太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於沿岸淺水域的底質上，並在底層爬行尋找藻類或者生物的碎屑並以此為食。此類群缺少一般海星擁有用來清潔身體的的叉棘，此物種則是透過體表的纖毛來製造水流藉此清洗體表的附著物。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體呈五角形，腕較短，末端鈍圓，腹面平坦，背面略凸，背板小呈現半月或複葉形。背板上有細棘或小顆粒，皮鰓位於背部中間區域。腹板小，呈橫向或縱向及v字型排列，腹板上有單一或數個小棘。","strThumbnailUrl":null}]