[{"strDataType":"text","strName":"中文名","strValue":"骨雀鱔","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Lepisosteus osseus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"雀鱔科 (Lepisosteidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"200 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"美洲至中美洲","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"棲息於淡水湖泊或大型河流，且常躲藏於水生植物周邊。偶爾會被發現產卵或棲息於鹹水湖泊，沉性卵。為強勢的掠食魚種，以其他魚類或甲殼類為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體長，成圓柱狀，口吻狹長，背鰭無硬棘，軟條6-9，臀鰭無硬棘，軟條8-10，鱗片堅硬且具陶瓷光澤，背鰭及臀鰭與身體連接處具骨質鱗片，尾鰭兩側連接處同樣具骨質鱗片。","strThumbnailUrl":null},{"strDataType":"video","strName":"骨雀鱔（長吻雀鱔）","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1942/d26a16af44323776052e1410d13e8ffb.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1942/preview.jpg"},{"strDataType":"image","strName":"三館動物89","strValue":"http://image.nmmba.gov.tw/upload/archive/4513/preview_004513.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4513/thumbnail_004513.jpg"},{"strDataType":"image","strName":"三館動物90","strValue":"http://image.nmmba.gov.tw/upload/archive/4514/preview_004514.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4514/thumbnail_004514.jpg"},{"strDataType":"image","strName":"三館動物91","strValue":"http://image.nmmba.gov.tw/upload/archive/4515/preview_004515.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4515/thumbnail_004515.jpg"},{"strDataType":"image","strName":"三館動物92","strValue":"http://image.nmmba.gov.tw/upload/archive/4516/preview_004516.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4516/thumbnail_004516.jpg"},{"strDataType":"image","strName":"三館動物93","strValue":"http://image.nmmba.gov.tw/upload/archive/4517/preview_004517.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/4517/thumbnail