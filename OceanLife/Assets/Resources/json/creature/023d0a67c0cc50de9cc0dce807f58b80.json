[{"strDataType":"text","strName":"中文名","strValue":"黃帶龍占魚","strThumbnailUrl":null},{"strDataType":"text","strName":"學名","strValue":"Lethrinus ornatus","strThumbnailUrl":null},{"strDataType":"text","strName":"中英文科名","strValue":"龍占魚科 (Lethrinidae)","strThumbnailUrl":null},{"strDataType":"text","strName":"中文俗名","strValue":"龍尖","strThumbnailUrl":null},{"strDataType":"text","strName":"英文俗名","strValue":"Ornate emperor, Yellow-striped emperor","strThumbnailUrl":null},{"strDataType":"text","strName":"最大體長","strValue":"45 cm","strThumbnailUrl":null},{"strDataType":"text","strName":"臺灣分布","strValue":"東部、西部、南部、澎湖、東沙 ","strThumbnailUrl":null},{"strDataType":"text","strName":"世界分布","strValue":"印度至西太平洋","strThumbnailUrl":null},{"strDataType":"text","strName":"棲所生態","strValue":"主要棲息於潟湖、內灣、珊瑚礁區或海草床，或其外緣砂地上巡游，以砂地上之甲殼類、軟體、棘皮動物、多毛類或小魚等動物為食。","strThumbnailUrl":null},{"strDataType":"text","strName":"形態特徵","strValue":"體延長而呈長橢圓形。吻短而鈍，吻上緣與上頜間的角度為64°-73°。眼間隔凸起。眼大，位於近於頭背側。口端位；兩頜具犬齒及絨毛狀齒，後方側齒呈圓形而有犬齒尖或臼齒但呈塊狀；上頜骨上緣平滑或稍呈鋸齒狀。頰部無鱗；胸鰭基部內側具鱗；側線鱗數46-47；側線上鱗列數5.5；側線下鱗列數16-17。背鰭單一，不具深刻，具硬棘X，軟條9，第IV或V棘最長；臀鰭硬棘III，軟條8，第1軟條通常最長，但等於或短於軟條部之基底長；胸鰭軟條13；尾鰭分叉，兩葉先端尖型。體呈淺黃褐色，體側有5-6條黃色或橙色縱帶；鰓蓋緣及眼緣上下有少許紅色斑。背、尾鰭呈朱紅色；胸、腹及臀鰭黃色。","strThumbnailUrl":null},{"strDataType":"video","strName":"黃帶龍占","strValue":"http://oceanvideo.tapmovie.com/assets/enclosures/1958/d5d2fb7802863244721fdcb80fe6789d.mp4","strThumbnailUrl":"http://oceanvideo.tapmovie.com/assets/enclosures/1958/preview.jpg"},{"strDataType":"image","strName":"黃帶龍占","strValue":"http://image.nmmba.gov.tw/upload/archive/9311/preview_20100726155743.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/9311/thumbnail_20100726155743.jpg"},{"strDataType":"image","strName":"黃帶龍占","strValue":"http://image.nmmba.gov.tw/upload/archive/9918/preview_20120106113958.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/9918/thumbnail_20120106113958.jpg"},{"strDataType":"image","strName":"黃帶龍占","strValue":"http://image.nmmba.gov.tw/upload/archive/14073/preview_20120704090925.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/14073/thumbnail_20120704090925.jpg"},{"strDataType":"image","strName":"黃帶龍占 ","strValue":"http://image.nmmba.gov.tw/upload/archive/14134/preview_20120710102821.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/14134/thumbnail_20120710102821.jpg"},{"strDataType":"image","strName":"黃帶龍占 ","strValue":"http://image.nmmba.gov.tw/upload/archive/16146/preview_20130513145209.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/16146/thumbnail_20130513145209.jpg"},{"strDataType":"image","strName":"黃帶龍占 ","strValue":"http://image.nmmba.gov.tw/upload/archive/22129/preview_20160803111604.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22129/thumbnail_20160803111604.jpg"},{"strDataType":"image","strName":"黃帶龍占 ","strValue":"http://image.nmmba.gov.tw/upload/archive/22153/preview_20160808162508.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22153/thumbnail_20160808162508.jpg"},{"strDataType":"image","strName":"1433.\t短吻裸頰鯛 Lethrinus ornatus Valenciennes, 1830","strValue":"http://image.nmmba.gov.tw/upload/archive/22200/preview_20160815092633.jpg","strThumbnailUrl":"http://image.nmmba.gov.tw/upload/archive/22200/thumbnail_20160815092633.jpg"}]